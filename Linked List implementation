#include<iostream>
#include<bits/stdc++.h>
using namespace std;
struct Node{
int data;
Node* next;
};
Node* head;
void enqueue(int x){
Node* temp1=new Node;
temp1->data=x;
temp1->next=nullptr;
if(head==nullptr){
head=temp1;
return;
}
Node* temp2=head;
while(temp2->next!=nullptr)
temp2=temp2->next;

temp2->next=temp1;
};
void dequeue(){
Node* temp=head;
head=temp->next;
free(temp);
};
void print(){
Node* temp=head;
cout<<"Queue is: ";
while(temp!=nullptr){
cout<<temp->data<<" ";
temp=temp->next;
}
cout<<endl;
};
int main(){
head=nullptr;
//cout<<1<<" "; 
enqueue(1);
enqueue(2);
enqueue(3);
enqueue(4);
//cout<<1<<" ";
print();
dequeue();
//cout<<2<<" ";
print();
return 0;
}

/*
Queue is: 1 2 3 4 
Queue is: 2 3 4
*/
